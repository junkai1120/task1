class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        if needle in haystack:                    #如果haystack中有needle
            return haystack.index(needle)        #index()方法检测字符串haystask中是否包含子字符串needle，如果指定开始和结束范围，则检查是否包含在指定范围内。
        else:
            return -1
